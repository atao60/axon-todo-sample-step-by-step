<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="Stdout" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%-5p - %-10.10t - %-26.26c{1} - %m%n</pattern>
    </encoder>
  </appender>
  <logger name="org.axonframework.serializer.ChainingConverterFactory" level="WARN"/>
  <root level="WARN">
    <appender-ref ref="Stdout"/>
  </root>
</configuration>
