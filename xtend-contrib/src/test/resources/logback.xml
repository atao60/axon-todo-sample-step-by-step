<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="Stdout" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%-5p - %-10.10t - %-26.26c{1} - %m</pattern>
    </encoder>
  </appender>
  <root level="INFO">
    <appender-ref ref="Stdout"/>
  </root>
</configuration>
